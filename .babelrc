{
  "presets": [
    ["env", {
      "targets": {
        "node": "current" 
      }
    }],
    "react"
  ],
  "plugins": [
    [
      "babel-plugin-root-import", [
        {
          "rootPathSuffix": "src"
        }
      ]
    ],
    [
      "transform-runtime", {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-react-jsx-source"
  ]
}
